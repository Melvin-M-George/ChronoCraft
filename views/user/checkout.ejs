                                        </div>
                                    </div>

                                    <br>
                                    <div>
                                        <% if(product){ %>

                                            <a href="/couponList?id=<%= product._id %>" class="btn btn-success">Show
                                                Available coupons</a>

                                            <% }else{ %>

                                                <a href="/couponList" class="btn btn-success">Show
